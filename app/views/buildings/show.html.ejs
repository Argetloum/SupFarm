<div class="hero-unit">
  <h2 class="id"><%= building.id %></h2>
  <%- linkTo('Edit this building', editBuildingPath(params.id), {class: 'btn pull-right'}); %>
</div>

<h2>Building Properties</h2>
<% for(var i in building) { -%>
<% if(i !== 'saved') { -%>
<div class="row list-item">
  <div class="span4">
    <h3><%= i %></h3>
  </div>
  <div class="span8">
    <h3><i class="icon-list-alt"></i><span class="<%= i %>"><%= building[i] %></span></h3>
  </div>
</div>
<% } -%><% } -%>